  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ViewProfileControl</servlet-name>
    <url-pattern>/profile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AziendeViewControl</servlet-name>
    <url-pattern>/aziende</url-pattern>
  </servlet-mapping>
